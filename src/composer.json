{
    "require": {
        "php": "^8.1",
        "roots/acorn": "^3.2",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.6",
        "laravel/pint": "^v1.10.5",
        "fzaninotto/faker": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "CommunityHive\\App\\": "app",
            "CommunityHive\\Database\\Factories\\": "database/factories/",
            "CommunityHive\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "post-autoload-dump": [
        "Roots\\Acorn\\ComposerScripts::postAutoloadDump"
    ]
}
